<%- include("partials/header") %> 
<div class="container">
    <div class="registered-players-section">
    <table class="table">
        <thead class="thead-dark">
          <tr>
            <th scope="col">ID</th>
            <th scope="col">Jméno</th>
            <th scope="col">email</th>
            <th scope="col">Věk</th>
            <th scope="col">Kolej 1</th>
            <th scope="col">Kolej 2</th>
            <th scope="col">Login</th>
            <th scope="col"></th>
          </tr>
        </thead>
        <tbody>
        <% let count = 1 %>
        <% players.forEach(player => { %>
        <tr id="player<%-count%>">
            <th scope="row"><%-count%></th>
            <td><%-player.name%></td>
            <td><%-player.email%></td>
            <td><%-player.age%></td>
            <td><%-player.house1%></td>
            <td><%-player.house2%></td>
            <td><%-player.login%></td>
            <td><a href="/registered-players/<%=player.login%>&<%=count%>&<%=player.email%>"><i class="far fa-trash-alt" style="color: #e94560;"></i></a></td>
        </tr>
        
        <% count++; %>
        <%});%>


        </tbody>
      </table>
    </div>
</div>




<%- include("partials/footer") %> 